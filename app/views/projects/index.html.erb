<h1>List of projects</h1>

<%= link_to 'New project', new_project_path%>
<table>
  <tr>
    <th>Title</th>

    <th>Author</th>
  </tr>

  <% @projects.each do |project| %>
    <tr>
      <td><%= project.title %></td>    
      <td><%= project.author %></td>
      <td><%= link_to 'Show', project_path(project) %></td>
    </tr>
  <% end %>
</table>
