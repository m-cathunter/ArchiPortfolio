<%= form_for ([@project, @project.reviews.build]) do |f| %>

  <div id="rating-form">

    <label>Rating</label>

  </div>

  <p>
  <%= f.label :comment %>
  <%= f.text_area :comment %>
  </p>

  <p>
  <%= f.submit %>
  </p>

<% end %>

<script>
	$('#rating-form').raty({
		path: '/assets/',
		scoreName: 'review[rating]'
	});
</script>
